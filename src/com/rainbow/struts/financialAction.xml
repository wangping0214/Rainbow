<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
 <struts>
<!--******************************** 财务管理******************************************* -->
   <package name="financial" extends="struts-default">
   		<action name="initFinancial" class="financialAction" method="initFinancial">
   			<result name="success">financial.jsp</result>
   			<result name="error">financialError.jsp</result>
   		</action>
   		<action name="financialLogin"  class="financialAction" method="financialLogin"></action>
   		<action name="checkOutAppFinancial" class="financialAction" method="checkOutAppFinancial"></action>
   		<action name="exportFinancial" class="financialAction" method="exportFinancial"></action>
   </package>
</struts>