<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
 <struts>
<!--********************************查询app信息******************************************* -->
    <package name="UploadAVPhotosa" extends="struts-default">
    	<!-- 管理员上传广告图片 -->
    	<action name="UploadPhotos" class="UploadAVPhotos" method="UploadPhotos">
    	
    	</action>
    	<!-- 分页显示广告图片 -->
    	<action name="Search" class="UploadAVPhotos" method="Search">
    	<result>uploadPhotos.jsp</result>
    	</action>
    	<!-- 删除广告图 -->
    	<action name="DeleteImg" class="UploadAVPhotos" method="DeleteImg">
    	<result>uploadPhotos.jsp</result>
    	</action>
    	<!-- 管理广告图 -->
    	<action name="ADVn" class="UploadAVPhotos" method="ADVn">
    	<result>uploadPhotos.jsp</result>
    	</action>
    	<!-- 客户端通过type获取广告图 -->
    	<action name="ADVType" class="UploadAVPhotos" method="ADVType">
    	
    	</action>
    	
        </package>
</struts>
